<section class="team-section-{{section.id}}">
  <div class="team-section__heading">
    {% unless section.settings.title == blank %}
    <h2>{{ section.settings.title }}</h2>
    {% endunless %}
    {% unless section.settings.content == blank %}
    <div>{{ section.settings.content }}</div>
    {% endunless %}
    <div></div>
  </div>
  <div class="team-section__members">
    {% for block in section.blocks %}
    {% if block.type == "team_member" %}
    <div class="team-section__member" id="team-section__member-{{forloop.index}}">
      <div class="team-section__member-image"><img src={{ block.settings.member_image | img_url: "300x" }}></div>
      {% if block.settings.member_hover_image != blank %}
      <div class="team-section__member-hover-image"><img src={{ block.settings.member_hover_image | img_url: "300x" }}></div>
      {% endif %}
      <div class="team-section__member-content">
        <h2 class="team-section__member-name">{{ block.settings.member_name }}</h2>
        <p class="team-section__member-bio">{{ block.settings.member_bio }}</p>
        <div class="team-section__member-socials" style="display: none">
          {% unless block.settings.member_social_linkedin == blank %}
          <a href="{{ block.settings.member_social_linkedin }}">
            <svg width="30" zoomAndPan="magnify" viewBox="0 0 37.5 37.499999" height="30"><defs><clipPath id="linkedinid1"><path d="M 0 0 L 37.007812 0 L 37.007812 37.007812 L 0 37.007812 Z M 0 0 " clip-rule="nonzero"/></clipPath></defs><g clip-path="url(#linkedinid1)"><g transform="matrix(0.661017, 0, 0, 0.657895, 0.0000005, 0)"><image x="0" y="0" width="59" xlink:href="data:image/png;base64," xlink:type="simple" xlink:actuate="onLoad" height="57" preserveAspectRatio="xMidYMid meet" xlink:show="embed"/></g></g></svg>
          </a>
          {% endunless %}
          {% unless block.settings.member_social_facebook == blank %}
          <a href="{{ block.settings.member_social_facebook }}">
            <svg width="30" zoomAndPan="magnify" viewBox="0 0 37.5 37.499999" height="30" preserveAspectRatio="xMidYMid meet" version="1.0"><defs><clipPath id="facebookid1"><path d="M 0 0 L 37.007812 0 L 37.007812 37.007812 L 0 37.007812 Z M 0 0 " clip-rule="nonzero"/></clipPath></defs><g clip-path="url(#facebookid1)"><g transform="matrix(0.657895, 0, 0, 0.659483, 0.0000005, 0)"><image x="0" y="0" width="57" xlink:href="data:image/png;base64," xlink:type="simple" xlink:actuate="onLoad" height="58" preserveAspectRatio="xMidYMid meet" xlink:show="embed"/></g></g></svg>
          </a>
          {% endunless %}
          {% unless block.settings.member_social_instagram == blank %}
          <a href="{{ block.settings.member_social_instagram }}">
            <svg width="30" zoomAndPan="magnify" viewBox="0 0 37.5 37.499999" height="30" preserveAspectRatio="xMidYMid meet" version="1.0"><defs><clipPath id="instagramid1"><path d="M 0 0 L 37.007812 0 L 37.007812 37.007812 L 0 37.007812 Z M 0 0 " clip-rule="nonzero"/></clipPath></defs><g clip-path="url(#instagramid1)"><g transform="matrix(0.672414, 0, 0, 0.669643, -0.669642, 0)"><image x="0" y="0" width="58" xlink:href="data:image/png;base64," xlink:type="simple" xlink:actuate="onLoad" height="56" preserveAspectRatio="xMidYMid meet" xlink:show="embed"/></g></g></svg>
          </a>
          {% endunless %}
          {% unless block.settings.member_social_twitter == blank %}
          <a href="{{ block.settings.member_social_twitter }}">
            <svg width="30" zoomAndPan="magnify" viewBox="0 0 37.5 37.499999" height="30" preserveAspectRatio="xMidYMid meet" version="1.0"><defs><clipPath id="twitterid1"><path d="M 0 0 L 37.007812 0 L 37.007812 37.007812 L 0 37.007812 Z M 0 0 " clip-rule="nonzero"/></clipPath></defs><g clip-path="url(#twitterid1)"><path fill="rgb(100%, 100%, 100%)" d="M 0 0 L 37.5 0 L 37.5 45 L 0 45 Z M 0 0 " fill-opacity="1" fill-rule="nonzero"/><path fill="rgb(100%, 100%, 100%)" d="M 0 0 L 37.5 0 L 37.5 37.5 L 0 37.5 Z M 0 0 " fill-opacity="1" fill-rule="nonzero"/><path fill="rgb(100%, 100%, 100%)" d="M 0 0 L 37.5 0 L 37.5 37.5 L 0 37.5 Z M 0 0 " fill-opacity="1" fill-rule="nonzero"/><path fill="rgb(100%, 100%, 100%)" d="M 0 0 L 37.5 0 L 37.5 37.5 L 0 37.5 Z M 0 0 " fill-opacity="1" fill-rule="nonzero"/><g transform="matrix(0.646552, 0, 0, 0.646552, 0.0000005, 0)"><image x="0" y="0" width="58" xlink:href="data:image/png;base64," xlink:type="simple" xlink:actuate="onLoad" height="58" preserveAspectRatio="xMidYMid meet" xlink:show="embed"/></g></g></svg>
          </a>
          {% endunless %}
        </div>
        <div class="team-section__member-desc" style="display: none;">{{ block.settings.member_desc }}</div>
      </div>
    </div>
    {% elsif block.type == "heading" %}
    <div class="team-section__heading">
      {% unless block.settings.heading == blank %}
      <h2>{{ block.settings.heading }}</h2>
      {% endunless %}
      {% unless block.settings.sub_heading == blank %}
      <p>{{ block.settings.sub_heading }}</p>
      {% endunless %}
    </div>
    {% endif %}
    {% endfor %}
  </div>
</section>
<script>
  window.addEventListener('load', function() {
  	let teamMemberImgs = document.querySelectorAll('.team-section__member-image, .team-section__member-hover-image');
    teamMemberImgs.forEach(teamMemberImg => {
      teamMemberImg.addEventListener('click', function(e) {
        let memberDesc = teamMemberImg.closest('.team-section__member').querySelector('.team-section__member-desc').innerHTML;
        let memberName = teamMemberImg.closest('.team-section__member').querySelector('.team-section__member-name').innerHTML;
        let memeberBio = teamMemberImg.closest('.team-section__member').querySelector('.team-section__member-bio').innerHTML;
        let memberSocials = teamMemberImg.closest('.team-section__member').querySelector('.team-section__member-socials').innerHTML;
        const fancybox = new Fancybox([
          {
            src: `<div class="team-section__member-popup">
					<div class="team-section__member-popup-left"><div>${teamMemberImg.innerHTML}</div><div style="margin-top: 2em;"><h2 style="line-height: 1;margin-bottom: 0.25em;">${memberName}</h2><p>${memeberBio}</p></div><div>${memberSocials}</div></div>
					<div class="team-section__member-popup-right">${memberDesc}</div>
  				  </div>`,
            type: "html",
          },
        ]);
      });
    });
  });
</script>
<style>
  #shopify-section-{{section.id}} {
  	margin-top: 50px;
  }
  
  #shopify-section-{{section.id}} .team-section__heading {
    text-align: center;
    margin-bottom: 1em;
    margin-top: 3em;
    width: 100%;
  }
  #shopify-section-{{section.id}} .team-section__heading h2 {
  	font-size: 3rem;
    font-weight: bold;
    line-height: 1;
  }
  #shopify-section-{{section.id}} .team-section__members {
  	display: flex;
    align-items: flex-start;
    justify-content: flex-start;
    flex-wrap: wrap;
    max-width: 1080px;
    margin-left: auto;
    margin-right: auto;
  }
  
  #shopify-section-{{section.id}} .team-section__members .team-section__member {
  	width: calc(100% / 3);
    padding-left: 10px;
    padding-right: 10px;
    margin-bottom: 30px;
    position: relative;
  }
  
  #shopify-section-{{section.id}} .team-section__members .team-section__member:hover .team-section__member-hover-image,
  #shopify-section-{{section.id}} .team-section__members .team-section__member:focus .team-section__member-hover-image {
  	z-index: 1;
    opacity: 1;
    visibility: visible;
  }
  
  #shopify-section-{{section.id}} .team-section__members .team-section__member-content {
  	margin-top: 20px;
    max-width: 300px;
    margin-left: auto;
    margin-right: auto;
  }
  
  #shopify-section-{{section.id}} .team-section__members .team-section__member-content .team-section__member-name {
  	font-weight: bold;
    margin-bottom: 5px;
  }
  
  #shopify-section-{{section.id}} .team-section__members .team-section__member-content .team-section__member-bio {
  	line-height: 1.5;
  }
  
  #shopify-section-{{section.id}} .team-section__member-hover-image {
    position: absolute;
    top: 0;
    left: 50%;
    transform: translateX(-50%);
    width: 100%;
    max-width: 300px;
    z-index: -1;
    opacity: 0;
    visibility: hidden;
    transition: all ease-in-out .2s;
    cursor: pointer;
  }
  
  #shopify-section-{{section.id}} .team-section__member-hover-image img {
  	margin-top: 0;
  }
  
  #shopify-section-{{section.id}} .team-section__member-image img {
    margin: 0 auto 0 auto;
    transition: transform ease .6s;
  }
  
  #shopify-section-{{section.id}} .team-section__member-image {
/* 	background: linear-gradient(45deg, #ff5500, transparent); */
    overflow: hidden;
    cursor: pointer;
  }
  
/*   #shopify-section-{{section.id}} .team-section__member-image:hover img {
  	transform: scale(1.2);
  } */
  
  .fancybox__content {
  	border-radius: 5px;
  }
  
  .fancybox__content :focus:not(.carousel__button.is-close) {
    outline: none !important;
    box-shadow: none;
  }
  
  .team-section__member-popup {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    max-width: 800px;
    border-radius: 5px;
  }
  
  .team-section__member-popup .team-section__member-popup-left {
	width: 300px;
  }
  
  .team-section__member-popup .team-section__member-popup-left h2 {
  	font-size: 24px;
    font-weight: bold;
  }
  
  .team-section__member-popup .team-section__member-popup-right {
    width: calc(100% - 320px);
    padding-top: 60px;
  }
  
  .team-section__member-popup .team-section__member-popup-right strong {
    display: block;
    margin-bottom: -14px;
  }
  
  @media screen and (max-width: 680px) {
    #shopify-section-{{section.id}} .team-section__members .team-section__member {
      width: 100%;
    }
    
    .team-section__member-popup {
      flex-wrap: wrap;
    }
    
    .team-section__member-popup .team-section__member-popup-right {
      padding-top: 10px;
    }
    
    .team-section__member-popup .team-section__member-popup-left,
    .team-section__member-popup .team-section__member-popup-right {
      width: 100%;
    }
  }
</style>
{% schema %}
  {
    "name": "Team",
    "settings": [
		{
			"type": "text",
			"id": "title",
			"label": "Heading"
		},
		{
			"type": "textarea",
			"id": "content",
			"label": "Content"
		}
	],
	"blocks": [
		{
			"name": "Heading",
			"type": "heading",
			"settings": [
				{
					"type": "text",
					"id": "heading",
					"label": "Heading"
				},
				{
					"type": "text",
					"id": "sub_heading",
					"label": "Subheading"
				}
			]
		},
		{
			"name": "Team member",
			"type": "team_member",
			"settings": [
				{
					"type": "image_picker",
					"id": "member_image",
					"label": "Team member image"
				},
				{
					"type": "image_picker",
					"id": "member_hover_image",
					"label": "Team member hover image"
				},
				{
					"type": "text",
					"id": "member_name",
					"label": "Name"
				},
				{
					"type": "text",
					"id": "member_bio",
					"label": "Bio"
				},
				{
					"type": "richtext",
					"id": "member_desc",
					"label": "Description"
				},
				{
					"type": "url",
					"id": "member_social_linkedin",
					"label": "Linkedin"
				},
				{
					"type": "url",
					"id": "member_social_facebook",
					"label": "Facebook"
				},
				{
					"type": "url",
					"id": "member_social_instagram",
					"label": "Instagram"
				},
				{
					"type": "url",
					"id": "member_social_twitter",
					"label": "Twitter"
				}
			]
		}
	]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

